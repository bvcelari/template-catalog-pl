@Library('es-shared-library@dev')_

pipeline {
  parameters {
        string(name: 'a_different_param', defaultValue: 'Mr Jenkins', description: 'Who should I say hello to?')
    }
    agent any
    stages {
        stage('Example') {
            steps {
                echo "Hello ${params.a_different_param}"
            }
        }
    }
}
